BL_TEST_INTEGRATION_SECRET_JSON={"apiKey":"sk-..."}

# Optional: Integration configuration (JSON format)
# BL_TEST_INTEGRATION_CONFIG_JSON={"endpoint":"https://api.openai.com/v1"}

# OpenAI API Key (for future AI agent integration)
OPENAI_API_KEY=sk-...
